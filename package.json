{
  "name": "poc-rogue-sound-web",
  "version": "0.0.1",
  "description": "The best social DJ site where users can share music by tuning in to user-generated playlists created by music lovers from all over the world. Made with Spotify API.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/rogue-sound/poc-rogue-sound-web.git"
  },
  "keywords": [
    "spotify",
    "dj"
  ],
  "author": "Everfriends",
  "license": "ISC",
  "scripts": {
    "build-dev": "webpack -d --mode development",
    "build-prod": "webpack -p --mode production",
    "clean": "rm dist/bundle.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prettify": "prettier --write \"src/**/*.js\"",
    "start": "webpack-dev-server --hot --mode development",
    "storybook": "start-storybook",
    "extract-messages": "cross-env NODE_ENV=development extract-messages -l=en,es -o src/translations -d en --flat true 'src/**/*.js'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
